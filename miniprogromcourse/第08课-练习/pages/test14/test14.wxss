/* pages/test14/test14.wxss */
.bottom{
  position: fixed;
  bottom: 0
}
.modal-mask {
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  background: #000;
  opacity: 0.5;
  overflow: hidden;
  z-index: 9000;
  color: #fff;
}
.modal-dialog{
  overflow: hidden;
  position: fixed;
  z-index: 9999;
  background: #f9f9f9;
  display: flex;
  flex-direction: column
}
.vline{
  display: flex;
  background: #fff;
  z-index: 10000;
  position: fixed;
  width: 100%;
  bottom: 200px
}
.vleft{
   flex: 1;
   margin: 15rpx;
   color: #666666
}
.vright{
   flex: 1;
   text-align: right;
   margin: 15rpx;
   color: #1AAD19
}

